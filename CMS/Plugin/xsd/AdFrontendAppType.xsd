  <xsd:complexType name="DebugInfoParamsType">
    <xsd:annotation>
      <xsd:documentation>
        Debug information settings
      </xsd:documentation>
    </xsd:annotation>
    <xsd:attribute name="use_acl" type="xsd:boolean" default="true">
        <xsd:annotation>
          <xsd:documentation>
            Allow debug output for list of IP adresses. If unchecked, all adresses are allowed.
          </xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="acl" type="cms:Text" default="">
      <xsd:annotation>
        <xsd:documentation>
          List of allowed IP adresses for debug output.
          Leave empty to deny from all. Otherwise list allowed IP addresses, one per line.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="colocations" type="IntegerListType" default="">
      <xsd:annotation>
        <xsd:documentation>
          List of allowed colocations for debug output.
          Leave empty to allow from all. Otherwise list allowed colocation separated by comma.
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="show_history_profile" type="xsd:boolean" default="false">
        <xsd:annotation>
          <xsd:documentation>
            Show channel list from history profile in the debug output, it is unchecked by default.
          </xsd:documentation>
        </xsd:annotation>
    </xsd:attribute>
